{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://moonwalker.onrender.com/"
    }
  ],
  "build": {
    "env": {
      "VITE_API_URL": "https://moonwalker.onrender.com/"
    }
  },
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/build"
}
